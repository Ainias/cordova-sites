{
  "name": "cordova-sites",
  "displayName": "Cordova Sites",
  "version": "0.0.1",
  "description": "A Cordova JS/HTML/CSS-Framework similar to android (or at least i hope so).",
  "main": "dist/client.js",
  "scripts": {
    "eslint": "eslint 'src/client/**/*.tsx' 'src/shared/**/*.ts'",
    "build": "node bin/build.js && tsc && cd ./src && find . -regextype sed -regex '.*\\.\\(html\\|png\\|svg\\|json\\)' -type f -exec cp --parents {} ../dist \\;",
    "update packages": "node bin/updateCopies.js",
    "force-audit-fix": "npx npm-force-resolutions"
  },
  "license": "MIT",
  "peerDependencies": {
    "bootstrap": "^5.1.0",
    "js-helper": "git+https://github.com/Ainias/js-helper#0.7.5",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-bootstrap-mobile": "git+https://github.com/Ainias/react-bootstrap-mobile#0.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-react": "^7.25.1",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "ts-loader": "^8.3.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "react-table": "^7.7.0",
    "@types/react-table": "^7.7.9",
    "cordova-plugin-nativestorage": "^2.3.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-webpack": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-nativestorage": {},
      "cordova-plugin-x-toast": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "immer": "8.0.1",
    "browserslist": "4.16.6"
  }
}
